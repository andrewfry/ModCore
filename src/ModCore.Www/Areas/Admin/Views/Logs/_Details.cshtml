@model ModCore.ViewModels.Core.vLog


<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h4 class="modal-title">Log Details</h4>

</div>
<div class="modal-body">

    <div class="row">
        <label class="col-xs-12 col-sm-3">Date</label>
        <div class="col-xs-12 col-sm-9">
            @Model.InsertDate
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12 col-sm-3">Level</label>
        <div class="col-xs-12 col-sm-9">
            @Model.ErrorLevel
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12 col-sm-3">Message</label>
        <div class="col-xs-12 col-sm-9">
            @Model.Message
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12 col-sm-3">Error Message</label>
        <div class="col-xs-12 col-sm-9">
            @Model.ErrorMessage
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12">Inner Exception</label>
        <div class="col-xs-12">
            @Model.InnerException
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12">Stack Trace</label>
        <div class="col-xs-12">
            @Model.StackTrace
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12 col-sm-3"> Route</label>
        <div class="col-xs-12 col-sm-9">
            @Model.Route
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12 col-sm-3">Plugin</label>
        <div class="col-xs-12 col-sm-9">
            @Model.PluginName
        </div>
    </div>
    <div class="row">
        <label class="col-xs-12 col-sm-3">Class Name</label>
        <div class="col-xs-12 col-sm-9">
            @Model.ClassName
        </div>
    </div>

</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
</div>